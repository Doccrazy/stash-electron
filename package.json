{
  "name": "stash-electron",
  "productName": "Stash",
  "version": "0.0.1-dev",
  "description": "Cryptographically secure team password manager built on open technologies and standardized encryption algorithms.",
  "main": "./main.js",
  "scripts": {
    "clean": "rimraf build/*",
    "build": "concurrently \"yarn run build-main\" \"yarn run build-renderer\"",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.ts --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.ts --colors",
    "dev": "cross-env START_HOT=1 yarn run start-renderer-dev",
    "gen-icon": "node internals/scripts/generateIcons.js app/logo3.svg",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "yarn run clean && yarn run build && node -r ts-node/register internals/scripts/build.ts",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "yarn run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r ts-node/register ./app/main",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.ts",
    "test": "cross-env NODE_ENV=test TS_NODE_FILES=true mocha --trace-warnings -r ts-node/register -r test/register \"test/**/*.ts\"",
    "test-e2e": "cross-env NODE_ENV=test TS_NODE_FILES=true mocha --trace-warnings -r ts-node/register -r test/register \"test/e2e/**/*.ts\"",
    "test-watch": "yarn test -- --watch"
  },
  "browserslist": "electron 1.7",
  "build": {
    "productName": "Stash",
    "appId": "de.doccrazy.Stash",
    "files": [
      "package.json",
      {
        "from": "build",
        "to": ""
      },
      {
        "from": "app",
        "to": "",
        "filter": [
          "logo3.svg",
          "splash-screen.html"
        ]
      },
      "!node_modules",
      "node_modules/keytar/build/Release/keytar.node",
      "node_modules/nodegit/build/Release/nodegit.node"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "desktop": {
        "StartupWMClass": "Stash",
        "MimeType": "x-scheme-handler/stash;"
      }
    },
    "pacman": {
      "depends": [
        "gtk3",
        "libcurl-gnutls",
        "libsecret",
        "libxss",
        "nss"
      ]
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3",
        "libcurl3-gnutls",
        "libsecret-1-0"
      ],
      "packageCategory": "utils",
      "priority": "optional",
      "publish": {
        "provider": "bintray",
        "repo": "deb",
        "owner": "doccrazy",
        "publishAutoUpdate": false,
        "component": "main"
      }
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "protocols": {
      "name": "Stash URL",
      "schemes": [
        "stash"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Doccrazy/stash-electron.git"
  },
  "author": {
    "name": "Matthias Piepkorn",
    "email": "mpiepk@gmail.com",
    "url": "https://github.com/Doccrazy"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Doccrazy/stash-electron/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/Doccrazy/stash-electron#readme",
