    "@types/react-router-redux": "^5.0.8",
    "@types/reactstrap": "^5.0.3",
    "@types/redux-logger": "^3.0.3",
    "@types/source-map-support": "^0.4.0",
    "@types/ssh2-streams": "^0.1.2",
    "@types/sshpk": "^1.10.3",
    "bootstrap": "^4.0.0-beta",
    "classnames": "^2.2.5",
    "devtron": "^1.4.0",
    "diacritics": "^1.3.0",
    "electron-debug": "^1.2.0",
    "electron-log": "^2.2.11",
